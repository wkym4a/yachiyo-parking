<p id="notice"><%= notice %></p>

<h1>Parkings</h1>

<table>
  <thead>
    <tr>
      <th>Symbol</th>
      <th>Name</th>
      <th>Address</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Price</th>
      <th>Memo</th>
      <th>Managing memo</th>
      <th>Number</th>
      <th>Empty number</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkings.each do |parking| %>
      <tr>
        <td><%= parking.symbol %></td>
        <td><%= parking.name %></td>
        <td><%= parking.address %></td>
        <td><%= parking.lat %></td>
        <td><%= parking.lon %></td>
        <td><%= parking.price %></td>
        <td><%= parking.memo %></td>
        <td><%= parking.managing_memo %></td>
        <td><%= parking.number %></td>
        <td><%= parking.empty_number %></td>
        <td><%= parking.status %></td>
        <td><%= parking.user_id %></td>
        <td><%= link_to 'Show', parking %></td>
        <td><%= link_to 'Edit', edit_parking_path(parking) %></td>
        <td><%= link_to 'Destroy', parking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking', new_parking_path %>
